   "source": [
    "# Python3 program to insert an element into sorted list\n",
    "import bisect \n",
    "\n",
    "def insert(list, n):\n",
    "    bisect.insort(list, n) \n",
    "    return list\n",
    "\n",
    "list = [11, 22, 34]\n",
    "n = int(input(\"Enter the element to be inserted: \"))\n",
    "sl=insert(list, n)\n",
    "print(\"Sorted list is: \",sl)\n"
   ]
