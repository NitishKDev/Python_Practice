     "text": [
      "Enter numerator 3\n",
      "Enter denominator 0\n",
      "Denominator cannot be zero\n"
     ]
    }
   ],
   "source": [
    "nume=int(input(\"Enter numerator \"))\n",
    "deno=int(input(\"Enter denominator \"))\n",
    "try:\n",
    "    res=nume/deno\n",
    "    print(res)\n",
    "except ZeroDivisionError:\n",
    "    print(\"Denominator cannot be zero\")"
   ]
